{
    "version": "2.0.0",
    "command": "g++",
    "args": [
        "-g", 
        "-std=c++11", 
        "${file}", 
        "-o", 
        "${fileDirname}/build/${fileBasenameNoExtension}.exe",  
        "-I", "D:\\Opencv\\build\\include",
        "-I", "D:\\Opencv\\build\\include\\opencv2",
        "-L", "D:\\Opencv\\build\\x64\\MinGW\\lib",
        "-l", "libopencv_core420",
        "-l", "libopencv_imgproc420",
        "-l", "libopencv_imgcodecs420",
        "-l", "libopencv_video420",
        "-l", "libopencv_ml420",
        "-l", "libopencv_highgui420",
        "-l", "libopencv_objdetect420",
        "-l", "libopencv_flann420",
        "-l", "libopencv_imgproc420",
        "-l", "libopencv_photo420",
        "-l", "libopencv_videoio420"
  
    ],
    "problemMatcher":{
        "owner": "cpp",
        "fileLocation":[
            "relative",
            "${workspaceFolder}"
        ],
        "pattern":[
            {
                "regexp": "^([^\\\\s].*)\\\\((\\\\d+,\\\\d+)\\\\):\\\\s*(.*)$",
                "file": 1,
                "location": 2,
                "message": 3
            }
        ]
    },
    "group": {
        "kind": "build",
        "isDefault": true
    }
  }
